<idea-plugin>
    <id>org.jetbrains.plugins.compose.template</id>
    <name>Compose Template Example</name>
    <vendor url="https://www.yourcompany.com">YourCompany</vendor>

    <dependencies>
        <module name="intellij.platform.compose"/>
    </dependencies>

    <resource-bundle>messages.ComposeTemplate</resource-bundle>

    <extensions defaultExtensionNs="com.intellij">
        <toolWindow factoryClass="com.github.nebojsavuksic.intellijplatformcomposeplugintemplate.toolWindow.ComposeSamplesToolWindowFactory" id="ComposeSamplesToolWindow"/>
    </extensions>
</idea-plugin>
